<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <ikecode>
    <default>
      <logPath name="logPath"
        xdt:Transform="Replace"
        xdt:Locator="Match(name)"></logPath>
    </default>
    <verbose>
      <enabled name="verbose"
        xdt:Transform="Replace"
        xdt:Locator="Match(name)">true</enabled>
    </verbose>
    <exception>
      <enabled name="exception"
        xdt:Transform="Replace"
        xdt:Locator="Match(name)">true</enabled>
    </exception>
    <warning>
      <enabled name="warning"
        xdt:Transform="Replace"
        xdt:Locator="Match(name)">true</enabled>
    </warning>
    <metric>
      <enabled name="metric"
        xdt:Transform="Replace"
        xdt:Locator="Match(name)">true</enabled>
    </metric>
  </ikecode>
</configuration>